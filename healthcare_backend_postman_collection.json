{
  "info": {
    "name": "Healthcare Backend API",
    "description": "Postman collection for Django Healthcare Backend (auth, patients, doctors, mappings).",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://127.0.0.1:8000"
    },
    {
      "key": "token",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"demo\",\n  \"email\": \"demo@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/register/",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "register", ""]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"demo\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/login/",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "login", ""]
            }
          }
        }
      ]
    },
    {
      "name": "Patients",
      "item": [
        {
          "name": "Create Patient",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"age\": 30,\n  \"contact_number\": \"555-0000\",\n  \"address\": \"123 Main St\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/patients/",
              "host": ["{{base_url}}"],
              "path": ["api", "patients", ""]
            }
          }
        },
        {
          "name": "List Patients",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/patients/",
              "host": ["{{base_url}}"],
              "path": ["api", "patients", ""]
            }
          }
        },
        {
          "name": "Get Patient by ID",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/patients/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "patients", "1", ""]
            }
          }
        },
        {
          "name": "Update Patient",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Updated\",\n  \"age\": 31,\n  \"contact_number\": \"555-0001\",\n  \"address\": \"New Address\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/patients/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "patients", "1", ""]
            }
          }
        },
        {
          "name": "Delete Patient",
          "request": {
            "method": "DELETE",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/patients/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "patients", "1", ""]
            }
          }
        }
      ]
    },
    {
      "name": "Doctors",
      "item": [
        {
          "name": "Create Doctor",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Dr. Smith\",\n  \"specialization\": \"Cardiology\",\n  \"email\": \"smith@example.com\",\n  \"contact_number\": \"123-456-7890\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/doctors/",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", ""]
            }
          }
        },
        {
          "name": "List Doctors",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/doctors/",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", ""]
            }
          }
        },
        {
          "name": "Get Doctor by ID",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/doctors/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "1", ""]
            }
          }
        },
        {
          "name": "Update Doctor",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Dr. Smith Updated\",\n  \"specialization\": \"Cardiology\",\n  \"email\": \"smith2@example.com\",\n  \"contact_number\": \"999-999-9999\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/doctors/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "1", ""]
            }
          }
        },
        {
          "name": "Delete Doctor",
          "request": {
            "method": "DELETE",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/doctors/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "1", ""]
            }
          }
        }
      ]
    },
    {
      "name": "Mappings",
      "item": [
        {
          "name": "Create Mapping",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"patient\": 1,\n  \"doctor\": 1\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/mappings/",
              "host": ["{{base_url}}"],
              "path": ["api", "mappings", ""]
            }
          }
        },
        {
          "name": "List Mappings",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/mappings/",
              "host": ["{{base_url}}"],
              "path": ["api", "mappings", ""]
            }
          }
        },
        {
          "name": "List Mappings by Patient",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/mappings/?patient=1",
              "host": ["{{base_url}}"],
              "path": ["api", "mappings", ""],
              "query": [
                {"key": "patient", "value": "1"}
              ]
            }
          }
        },
        {
          "name": "Delete Mapping",
          "request": {
            "method": "DELETE",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{base_url}}/api/mappings/1/",
              "host": ["{{base_url}}"],
              "path": ["api", "mappings", "1", ""]
            }
          }
        }
      ]
    }
  ]
}
